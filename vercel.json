{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm ci --include=optional --audit=false",
  "nodeVersion": "20.x",
  "framework": null
}
